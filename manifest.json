{
    "name": "Whats App custom",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com//*"],
            "js": ["background.js"]
        }
    ],
    "options_ui": {
        "page": "optionsPage.html",
        "open_in_tab": false
    },
    "permissions": ["storage"]
  }